<%= render partial: 'api/v10/head', locals: { name: 'Energy', path: developers_data_category_path(data_category: 'energy') } %>

<div class="table-responsive">
<table class="table">
  <thead>
    <tr>
      <th>Year (March)</th>
      <th>Domestic</th>
      <th>Commercial</th>
      <th>Total</th>
    </tr>
  </thead>
  <tbody>
    <% @electricity_consumption.each do |f| %>
    <tr>
    <td><%= f['Year'] %></td>
    <td><%= f['Domestic'] %></td>
    <td><%= f['Commercial'] %></td>
    <td><%= f['Total'] %></td>
    </tr>
    <% end %>
  </tbody>
</table>
</div>